{
  "extends": [
    "./node_modules/gts/"
  ],
  "plugins": [
    "node"
  ],
  "rules": {
    "node/no-extraneous-import": [
      2,
      {
        "allowModules": [
          "prop-types",
          "react-router",
          "history",
          "moment",
          "@testing-library/dom"
        ]
      }
    ],
    "node/no-unpublished-import": [
      2,
      {
        "allowModules": [
          "redux-mock-store",
          "@testing-library/react",
          "@testing-library/jest-dom",
          "@testing-library/react-hooks"
        ]
      }
    ],
    "@typescript-eslint/no-unused-vars": [
      1,
      {
        "argsIgnorePattern": "^_"
      }
    ]
  }
}